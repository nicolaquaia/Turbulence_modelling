(
    down
    {
        type            symmetryPlane;
        inGroups        1(symmetryPlane);
        nFaces          100;
        startFace       6250;
    }
    right
    {
        type            patch;
        nFaces          30;
        startFace       6350;
    }
    up
    {
        type            symmetryPlane;
        inGroups        1(symmetryPlane);
        nFaces          100;
        startFace       6380;
    }
    left
    {
        type            patch;

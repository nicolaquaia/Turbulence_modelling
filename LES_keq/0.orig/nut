FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.0e-05;

boundaryField
{
    down
    {
        type            symmetry;
    }

    right
    {
        type            calculated;
        value           uniform 0;
    }

    up
    {
        type            symmetry;
    }

    left
    {
        type            calculated;
        value           $internalField;
    }

    cylinder
    {
        //type            nutUSpaldingWallFunction; //Change this to wall-resolved instead of modeled
        //value           $internalField;
        type            calculated;
        value           $internalField;
    }
    
    frontAndBack
    {
        type            symmetry;
    }
    

    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
